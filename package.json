{
  "name": "react-feedbacker",
  "version": "2.1.0",
  "description": "A minimal base for creating notifications as feedback in the UI",
  "main": "dist/index.js",
  "module": "dist/react-feedbacker.esm.js",
  "typings": "dist/index.d.ts",
  "author": "Viktor Ivarsson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/viktorivarsson/react-feedbacker.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "feedback",
    "notification",
    "notify",
    "toast"
  ],
  "scripts": {
    "test": "tsdx test --env=jsdom",
    "test:coverage": "tsdx test --env=jsdom --coverage",
    "start": "tsdx watch",
    "start:example": "cd example && npm start",
    "build": "rimraf dist && tsdx build",
    "ts:check": "tsc --noEmit",
    "lint": "eslint '**/*.{ts,tsx}'",
    "preversion": "npm test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "tsdx": "^0.14.1",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {}
}
